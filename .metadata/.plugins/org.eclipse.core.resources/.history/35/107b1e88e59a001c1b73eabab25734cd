	public static void main(String[] args) throws FileNotFoundException 
	
	{
		File plik = new File ("zakupy3.txt");
		Scanner sc = new Scanner (plik);
		System.out.println("sok 1 700\n"
				+ "jablka 4 500\n"
				+ "szynka 1 600\n"
				+ "bulki 2 200\n"
				+ "chlep 1 300\n"
				+ "banan 1 30\n"
				+ "maslo 1 800\n"
				+ "cukierek 1 20\n"
				+ "syr 1 1000\n"
				+ "gziby 10 600");
	}

}
